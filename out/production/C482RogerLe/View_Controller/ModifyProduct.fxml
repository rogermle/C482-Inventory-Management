                                <TableColumn fx:id="addProdInvColAssoc" prefWidth="109.0" text="Inventory Level" />
                                <TableColumn fx:id="addProdPartPriceColAssoc" prefWidth="129.0" text="Price/Cost pet Unit" />
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <Button id="addProductCancelBtn" fx:id="addProductsCancelButton" layoutX="693.0" layoutY="500.0" mnemonicParsing="false" onMouseClicked="#cancelAddProducts" prefHeight="25.0" prefWidth="72.0" text="Cancel" />
                <Button id="addProductSaveBtn" fx:id="addProdSaveButton" layoutX="610.0" layoutY="500.0" mnemonicParsing="false" onMouseClicked="#saveAddParts" prefHeight="25.0" prefWidth="65.0" text="Save" />
