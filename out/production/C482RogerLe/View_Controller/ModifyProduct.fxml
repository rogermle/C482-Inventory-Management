                </AnchorPane>
                <AnchorPane layoutX="415.0" layoutY="69.0" prefHeight="196.0" prefWidth="390.0">
                    <children>
                        <TextField fx:id="addProductsSearchBox" layoutX="162.0" layoutY="10.0" onMouseClicked="#clearTextField" prefHeight="25.0" prefWidth="149.0" />
                        <Button id="addProductSearchBtn" fx:id="addProdSearchButton" layoutX="319.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#searchParts" text="Search" />
                        <Button id="addProductAddBtn" fx:id="addProdAddButton" layoutX="275.0" layoutY="170.0" mnemonicParsing="false" onMouseClicked="#addParts" prefHeight="25.0" prefWidth="72.0" text="Add" />
