                        <TextField layoutX="140.0" layoutY="60.0" prefHeight="27.0" prefWidth="226.0" style="-fx-background-color: silver;" text="Auto Gen - Disabled" />
                        <TextField fx:id="name" layoutX="140.0" layoutY="100.0" onMouseClicked="#clearTextField" prefHeight="27.0" prefWidth="226.0" text="Product Name" />
                        <TextField fx:id="Inventory" layoutX="140.0" layoutY="140.0" onMouseClicked="#clearTextField" prefHeight="27.0" prefWidth="226.0" text="Inventory" />
